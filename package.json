{
  "name": "fcs-design-system",
  "version": "0.0.3",
  "description": "design system react components",
  "license": "MIT",
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Storybook"
  ],
  "homepage": "https://stembrino.github.io/fcs-design-system",
  "registry": "https://www.npmjs.com/package/fcs-design-system",
  "bugs": {
    "url": "https://github.com/stembrino/fcs-design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stembrino/fcs-design-system"
  },
  "author": "Fabio R. de Carvalho",
  "main": "dist/bundle.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "node ./_scripts/main.js",
    "build": "rm -fr dist/ && rollup -c",
    "prepublishOnly": "yarn test && yarn build",
    "generate": "node ./util/create-component",
    "gh:clean": "gh-pages-clean",
    "gh:page": "gh-pages -d storybook-static",
    "sb": "storybook dev",
    "sb:export": "storybook dist",
    "sb:build": "storybook build",
    "sb:deploy": "rm -fr storybook-static && yarn sb:build && gh-pages -d storybook-static",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "lint": "eslint src/**/* --ext \".ts,.tsx\"",
    "lint:fix": "eslint src/**/* --ext \".ts,.tsx\" --fix",
    "lint:css": "prettier --write \"src/**/*.css\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.10",
    "@babel/preset-env": "^7.22.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^8.0.0",
    "jest": "^29.5.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.0.18",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "class-variance-authority": "^0.6.0"
  }
}
